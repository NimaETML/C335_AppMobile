<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MauiUnitTests.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MauiUnitTests"
    xmlns:lib="clr-namespace:MauiUnitTests;assembly=MauiUnitTestsLib"
    Shell.FlyoutBehavior="Disabled"
    Title="MauiUnitTests">

    <Tab Title="Home">
        <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}" 
        Route="MainPage"/>
    </Tab>

    <Tab Title="MvvmIntPage">
        
        <ShellContent
        Title="MvvmIntPage"
        ContentTemplate="{DataTemplate lib:MvvmIntPage}" 
        Route="MvvmIntPage"/>
        
        <ShellContent
        Title="MainPage"
        ContentTemplate="{DataTemplate local:MainPage}" 
        Route="MainPage"/>
    </Tab>


</Shell>
