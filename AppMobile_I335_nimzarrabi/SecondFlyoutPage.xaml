<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="AppMobile_I335_nimzarrabi.SecondFlyoutPage" >
    <VerticalStackLayout
            Padding="30,0"
            Spacing="25">
        <Image
                Source="hungygames_palette.png"
                HeightRequest="185"
                Aspect="AspectFit"
                SemanticProperties.Description="Slay logo i made" />

        <Label
                Text="Flyout page 2"
                Style="{StaticResource Headline}"
                SemanticProperties.HeadingLevel="Level1" />

        <Label
                Text="this is the SECOND Flyout Page"
                Style="{StaticResource SubHeadline}"
                SemanticProperties.HeadingLevel="Level2"
                SemanticProperties.Description="this is the layout page, it has a cool image" />

    </VerticalStackLayout>
</ContentPage>
